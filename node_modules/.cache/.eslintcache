  value={requestTitle}\n              ref={title}\n              onChange={(e) => setTitle(e)}\n              onKeyDown={(e) => search(e)}\n              type=\"text\"\n              className=\"textInputField\"\n              placeholder=\"Телефоны, яблоки, груши...\"\n            />\n          </div>\n        </div>\n\n        <button onClick={(e) => getImages()} className=\"getFetch\">\n          {\" \"}\n          Искать\n        </button>\n      </div>\n      <div\n        style={{\n          display: noGetImages ? \"block\" : \"none\",\n        }}\n        className=\"noResult\"\n      >\n        <p> К сожалению, поиск не дал результатов </p>\n      </div>\n    </div>\n  );\n};\n\nexport default SearchContainer;\n","/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/redux/reducer/rootReducer.ts",[],[],"/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/hooks/useActions.ts",[],[],"/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/redux/reducer/imagesReducer.ts",[],[],"/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/redux/action-creators/action-creators.ts",["80"],[],"import * as asyncActions from './async-actions'\nimport * as imagesActions from './images-actions'\n\nexport default {\n    ...asyncActions,\n    ...imagesActions,\n}","/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/types/images.ts",[],[],"/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/redux/action-creators/async-actions.ts",[],[],"/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/redux/action-creators/images-actions.ts",["81","82"],[],"import { ImagesActionTypes } from './../../types/images';\nimport React from \"react\";\n\nexport const setRequesTitleAction = (title: string) => {\n    return {type: ImagesActionTypes.SET_REQUEST_TITLE, payload: title}\n}\n\nexport const isPaginationAction = () => {\n    {return {type: ImagesActionTypes.IS_PAGINATION,}}\n}\n\nexport const removeRequestTitleAction = () => {\n    return {type: ImagesActionTypes.REMOVE_REQUEST_TITLE}\n}\n\nexport const openModalAction = (image: any) => {\n    return {type: ImagesActionTypes.OPEN_MODAL, payload: image}\n}\n\nexport const closeModalAction = () => {\n    return {type: ImagesActionTypes.CLOSE_MODAL}\n}","/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/components/ModalImage.tsx",["83","84"],[],"import React, { useState } from \"react\";\nimport { useActions } from \"../hooks/useActions\";\nimport './ModalImage.css'\n\ninterface Image {\n    el: any,\n}\n\nconst ModalImage = (element: Image) => {\n    const {openModalAction} = useActions()\n    const elem = element.el\n\n    function setModal () {\n        openModalAction(elem)\n    }\n\n    return (\n      <div \n      onClick={ () => setModal()}\n      className=\"displayElement\" \n      style={{}}\n      key={elem.id}>\n        <img className=\"img\" src={elem.urls.regular} />\n      </div>\n    );\n}\n\nexport default ModalImage","/Users/romansehovcev/Documents/GitHub/test-project-sapsan/work-test-sapsan/src/components/ResultContainer.tsx",["85"],[],"import React from \"react\";\nimport { useActions } from \"../hooks/useActions\";\nimport { useTypedSelector } from \"../hooks/useTypedSelector\";\nimport \"./ResultContainer.css\";\nimport ModalImage from \"./ModalImage\";\n\nconst ResultContainer = () => {\n  const { images, modalImage } = useTypedSelector((state) => state.images);\n  const { closeModalAction } = useActions();\n\n  function closeModal() {\n    closeModalAction();\n  }\n\n  return (\n    <div className=\"DisplayResult\">\n      {images.map((el) => (\n        <ModalImage key={el.id} el={el} />\n      ))}\n      {modalImage !== null ? (\n        <div className=\"modalImageContainer\">\n          <img className=\"modalImage\" src={modalImage.urls.regular} />\n          <div onClick={